{
  "psfex_config_file_name":"default.psfex",
  "input_fits_format":"fit",
  "scamp_required_catalog_params":[
    "NUMBER",
    "EXT_NUMBER",
    "XWIN_IMAGE",
    "YWIN_IMAGE",
    "AWIN_IMAGE",
    "BWIN_IMAGE",
    "ERRAWIN_IMAGE",
    "ERRBWIN_IMAGE",
    "ERRTHETAWIN_IMAGE",
    "ERRA_WORLD",
    "ERRB_WORLD",
    "ERRTHETA_WORLD",
    "XWIN_WORLD",
    "YWIN_WORLD",
    "X_WORLD",
    "Y_WORLD",
    "FLUX_AUTO",
    "FLUX_MAX",
    "FLUX_RADIUS",
    "FLUXERR_AUTO",
    "MAG_AUTO",
    "MAGERR_AUTO",
    "FLAGS",
    "ELONGATION"
  ],
  "scamp_config_file_name":"default.scamp",
  "sex_config_file_name":"default.sex",
  "pipeline_steps":{
    "images_calibration":{
      "create_calibration_headers":{
        "scamp_kwargs":{
          "config_file":"default.scamp",
          "code":"SCAMP",
          "config":{
            "ASTREF_BAND":"DEFAULT",
            "COMPUTE_PARALLAXES":"N",
            "MOSAIC_TYPE":"UNCHANGED",
            "SN_THRESHOLDS":"10.0,100.0",
            "CHECKPLOT_DEV":"NULL",
            "SOLVE_ASTROM":"Y",
            "ASTREF_CATALOG":"UCAC-3",
            "EXPOTIME_KEY":"EXPTIME",
            "PIXSCALE_MAXERR":"1.2",
            "FWHM_THRESHOLDS":"0.0,10.0",
            "STABILITY_TYPE":"INSTRUMENT",
            "SOLVE_PHOTOM":"Y",
            "CROSSID_RADIUS":"3.0"
          }
        }
      },
      "create_calibrated_images_catalogs":{
        "sex_kwargs":{
          "config_file":"default.sex",
          "code":"SExtractor",
          "config":{
            "FILTER":"Y",
            "DETECT_THRESH":"3",
            "ANALYSIS_THRESH":"3",
            "FILTER_NAME":"gauss_4.0_7x7.conv"
          },
          "temp_path":"temp",
          "params":[
            "NUMBER",
            "EXT_NUMBER",
            "XWIN_IMAGE",
            "YWIN_IMAGE",
            "AWIN_IMAGE",
            "BWIN_IMAGE",
            "ERRAWIN_IMAGE",
            "ERRBWIN_IMAGE",
            "ERRTHETAWIN_IMAGE",
            "ERRA_WORLD",
            "ERRB_WORLD",
            "ERRTHETA_WORLD",
            "X_WORLD",
            "Y_WORLD",
            "XWIN_WORLD",
            "YWIN_WORLD",
            "FLUX_AUTO",
            "FLUX_MAX",
            "MAG_AUTO",
            "FLUXERR_AUTO",
            "FLAGS",
            "FLUX_RADIUS",
            "ELONGATION"
          ]
        }
      },
      "calibrate_images":{
        "swarp_kwargs":{
          "config_file":"default.swarp",
          "code":"SWarp",
          "config":{
            "CELESTIAL_TYPE":"NATIVE",
            "RESAMPLE_SUFFIX":".resamp.fits",
            "RESAMPLE":"N",
            "PROJECTION_TYPE":"NONE",
            "COMBINE":"Y",
            "RESAMPLE_DIR":"images",
            "DELETE_TMPFILES":"N"
          }
        }
      },
      "model_psf_calibrated_images_catalogs":{},
      "create_calibration_catalogs":{
        "sex_kwargs":{
          "config_file":"default.sex",
          "code":"SExtractor",
          "config":{
            "FILTER":"Y",
            "DETECT_THRESH":"5",
            "ANALYSIS_THRESH":"5",
            "FILTER_NAME":"gauss_4.0_7x7.conv"
          },
          "temp_path":"temp",
          "params":[
            "NUMBER",
            "EXT_NUMBER",
            "XWIN_IMAGE",
            "YWIN_IMAGE",
            "AWIN_IMAGE",
            "BWIN_IMAGE",
            "ERRAWIN_IMAGE",
            "ERRBWIN_IMAGE",
            "ERRTHETAWIN_IMAGE",
            "FLUX_AUTO",
            "FLUX_MAX",
            "MAG_AUTO",
            "FLUXERR_AUTO",
            "FLAGS",
            "FLUX_RADIUS",
            "ELONGATION"
          ]
        }
      },
      "create_calibrated_images_catalogs_psf":{}
    },
    "basic_reduction":{},
    "images_coaddition":{
      "model_psf_coadded_image_catalog":{},
      "coadd_calibrated_images":{
        "swarp_kwargs":{
          "config_file":"default.swarp",
          "code":"SWarp",
          "config":{
            "CELESTIAL_TYPE":"NATIVE",
            "RESAMPLE_SUFFIX":".resamp.fits",
            "RESAMPLE":"N",
            "PROJECTION_TYPE":"NONE",
            "COMBINE":"Y",
            "RESAMPLE_DIR":"temp",
            "DELETE_TMPFILES":"Y"
          }
        }
      },
      "create_coadded_image_catalog":{
        "sex_kwargs":{
          "config_file":"default.sex",
          "code":"SExtractor",
          "config":{
            "FILTER":"N",
            "DETECT_THRESH":"3",
            "ANALYSIS_THRESH":"3",
            "FILTER_NAME":"gauss_4.0_7x7.conv"
          },
          "temp_path":"temp",
          "params":[
            "NUMBER",
            "EXT_NUMBER",
            "XWIN_IMAGE",
            "YWIN_IMAGE",
            "AWIN_IMAGE",
            "BWIN_IMAGE",
            "ERRAWIN_IMAGE",
            "ERRBWIN_IMAGE",
            "ERRTHETAWIN_IMAGE",
            "ERRA_WORLD",
            "ERRB_WORLD",
            "ERRTHETA_WORLD",
            "X_WORLD",
            "Y_WORLD",
            "XWIN_WORLD",
            "YWIN_WORLD",
            "FLUX_AUTO",
            "FLUX_MAX",
            "MAG_AUTO",
            "FLUXERR_AUTO",
            "FLAGS",
            "FLUX_RADIUS",
            "ELONGATION"
          ]
        }
      },
      "create_coadded_image_catalog_psf":{}
    }
  },
  "swarp_config_file_name":"default.swarp"
}
